{
  "name": "@class101/ui",
  "version": "7.2.1",
  "description": "A React-based UI Component Library, powered by Class101.",
  "author": "Class101 Co, LTD.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pedaling/class101-ui.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "tsc -p . && rollup -c && docz build",
    "prebuild": "tsc -p . && rollup -c",
    "start": "rollup -c -w",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "semantic-release": "semantic-release",
    "generated-icon-export": "cd src/Icon && node generate-exports.js",
    "generated-svgr": "cd src/Icon/svgs && npx @svgr/cli --template ../svgr-cli.template.js --config-file ../.svgrrc  *.svg --out-dir ../generated --ext tsx",
    "svgr": "yarn generated-svgr && yarn generated-icon-export"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write --loglevel warn",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-parameters": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-template-literals": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@class101/eslint-config": "^2.2.1",
    "@class101/tslint-config": "^1.5.0",
    "@emotion/core": "^10.0.6",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.136",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "^4.4.0",
    "@types/swiper": "^4.4.5",
    "cross-env": "^5.1.4",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.12.9",
    "eslint": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^1.2.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.0",
    "prettier": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.24.2",
    "rollup-plugin-url": "^2.2.2",
    "rollup-plugin-visualizer": "^2.6.0",
    "semantic-release": "^15.13.1",
    "styled-components": "4.4.1",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "path-to-regexp": "^3.0.0",
    "polished": "^3.4.1",
    "swiper": "4.4.6"
  },
  "bugs": {
    "url": "https://github.com/pedaling/class101-ui/issues"
  },
  "homepage": "https://github.com/pedaling/class101-ui#readme",
  "keywords": [
    "ui-library",
    "ui-framework",
    "ui-components",
    "ui-kit",
    "ui-toolkit",
    "class101",
    "design"
  ]
}
