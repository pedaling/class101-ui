{
  "name": "@class101/ui",
  "version": "9.3.1",
  "description": "A React-based UI Component Library, powered by Class101.",
  "author": "Class101 Co, LTD.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pedaling/class101-ui.git"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "npm-publish": "yarn prebuild && yarn build && npm publish",
    "build": "docz build && dist-size ./dist",
    "prebuild": "tsc -p . && rollup -c",
    "analyze": "cross-env RUNNING_ENV=analyze yarn prebuild",
    "start": "rollup -c -w",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "generated-icon-export": "node src/Icon/tools/generate-export.js",
    "generated-svgr": "cd src/Icon/svgs && npx @svgr/cli --template ../tools/svgr-cli.template.js --config-file ../tools/.svgrrc *.svg --out-dir ../generated --ext tsx",
    "svgr": "yarn generated-svgr && yarn generated-icon-export"
  },
  "peerDependencies": {
    "lodash": "^4.17.13",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": ">=4 <6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@class101/eslint-config": "2.4.0",
    "@emotion/core": "^10.0.6",
    "@size-limit/preset-big-lib": "^4.5.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.136",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.8.3",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "babel-plugin-styled-components": "^1.11.1",
    "cross-env": "^5.1.4",
    "dist-size": "^0.4.0",
    "docz": "^2.3.1",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-theme-docz": "^2.3.1",
    "gh-pages": "^1.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^8.0.0",
    "prettier": "^2.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "2.50.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-url": "^3.0.1",
    "rollup-plugin-visualizer": "^4.0.4",
    "size-limit": "^4.5.0",
    "styled-components": "5.1.0",
    "tslib": "^1.9.3",
    "typescript": "3.8.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "path-to-regexp": "^3.0.0",
    "polished": "^3.4.1",
    "react-popper": "^1.3.7",
    "swiper": "6.7.0"
  },
  "bugs": {
    "url": "https://github.com/pedaling/class101-ui/issues"
  },
  "homepage": "https://github.com/pedaling/class101-ui#readme",
  "keywords": [
    "ui-library",
    "ui-framework",
    "ui-components",
    "ui-kit",
    "ui-toolkit",
    "class101",
    "design"
  ]
}
